{
    "manifest_version": 3,
    "name": "Job Info Filler",
    "description": "Job Application Info Filler",
    "version": "1.0",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "assets/images/auto-filler-icon.png",
            "32": "assets/images/auto-filler-icon.png",
            "48": "assets/images/auto-filler-icon.png",
            "128": "assets/images/auto-filler-icon.png"
        }
    },
    "icons": {
        "16": "assets/images/auto-filler-icon.png",
        "32": "assets/images/auto-filler-icon.png",
        "48": "assets/images/auto-filler-icon.png",
        "128": "assets/images/auto-filler-icon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}